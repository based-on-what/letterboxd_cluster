      "source": [
        "df = pd.read_csv('ratings.csv')  # columns: Name, Year, Rating (0.5‚Äì5)\n",
        "movie_data = []\n",
        "not_found = []\n",
        "no_genres = []\n",
        "\n",
        "for _, row in tqdm(df.iterrows(), total=len(df), desc='Fetching genres'):\n",
        "    name = row['Name']\n",
        "    year = int(row['Year'])\n",
        "    rating = float(row['Rating'])\n",
        "\n",
        "    print(f\"üîç Processing: {name} ({year})\")\n",
        "\n",
        "    mid = search_movie(name, year)\n",
        "    if not mid:\n",
        "        print(f\"‚ùå Movie not found: {name} ({year})\")\n",
        "        not_found.append(f\"{name} ({year})\")\n",
        "        continue\n",
        "\n",
        "    genres = get_movie_genres(mid)\n",
        "    if not genres:\n",
        "        print(f\"‚ö†Ô∏è Could not retrieve genres for: {name} ({year})\")\n",
        "        no_genres.append(f\"{name} ({year})\")\n",
        "        continue\n",
        "\n",
        "    movie_data.append({'Name': name, 'Year': year, 'Rating': rating, 'Genres': genres})\n",
        "    time.sleep(0.25)\n",
        "\n",
        "# Summary\n",
        "print(\"\\nüìä Processing complete!\")\n",
        "print(f\"‚úÖ Movies processed successfully: {len(movie_data)}\")\n",
        "print(f\"‚ùå Movies not found: {len(not_found)}\")\n",
        "print(f\"‚ö†Ô∏è Movies with no genres: {len(no_genres)}\")\n"
